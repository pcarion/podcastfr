{
  "name": "podcastfr",
  "version": "1.0.0",
  "description": "Liste de podcasts techniques en fran√ßais.",
  "main": ".eslintrc.js",
  "scripts": {
    "lint": "eslint src/**/*.ts",
    "clean": "rimraf dist build",
    "dist": "npm run clean && npm run build",
    "build": "tsc",
    "buildcss": "postcss css/styles.css -o static/assets/css/styles.css",
    "format": "prettier --write **/*.ts",
    "prevalidate": "npm run dist",
    "validate": "node build/validate/main.js",
    "pregenerate": "npm run dist && npm run buildcss",
    "generate": "node build/generate/index.js",
    "prepr": "npm run dist",
    "pr": "node build/pr/index.js",
    "live": "node build/live.js",
    "format-check": "prettier --check **/*.ts"
  },
  "dependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "autoprefixer": "^10.1.0",
    "axios": "^0.21.1",
    "fs-extra": "^9.0.1",
    "html-to-text": "^6.0.0",
    "js-yaml": "^3.14.1",
    "jtd": "0.0.9",
    "parse-diff": "^0.7.1",
    "postcss": "^8.2.2",
    "postcss-cli": "^8.3.1",
    "tailwindcss": "^2.0.2",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/fs-extra": "^9.0.6",
    "@types/html-to-text": "^6.0.0",
    "@types/js-yaml": "^3.12.5",
    "@types/live-server": "^1.2.0",
    "@types/node": "^14.14.17",
    "@types/xml2js": "^0.4.7",
    "@typescript-eslint/eslint-plugin": "^4.11.1",
    "@typescript-eslint/parser": "^4.11.1",
    "eslint": "^7.16.0",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-prettier": "^3.3.0",
    "live-server": "^1.2.1",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pcarion/podcastfr.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pcarion/podcastfr/issues"
  },
  "homepage": "https://github.com/pcarion/podcastfr#readme"
}
